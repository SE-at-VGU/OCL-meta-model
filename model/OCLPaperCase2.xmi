<?xml version="1.0" encoding="UTF-8"?>
<ocl:EIteratorExp
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ocl="http://www.example.org/ocl"
    xsi:schemaLocation="http://www.example.org/ocl ocl.ecore"
    kind="exists">
  <source xsi:type="ocl:EOperationCallExp"
      referredOperation="allInstances">
    <source
        xsi:type="ocl:ETypeExp"
        referredType="Car"/>
  </source>
  <body xsi:type="ocl:EOperationCallExp"
      referredOperation="&lt;>">
    <source
        xsi:type="ocl:EPropertyCallExp"
        referredProperty="color">
      <source
          xsi:type="ocl:EVariableExp"
          referredVariable="//@iterator.0"/>
    </source>
    <argument
        xsi:type="ocl:EStringLiteralExp"
        stringValue="no-color"/>
  </body>
  <iterator
      name="c"/>
</ocl:EIteratorExp>
